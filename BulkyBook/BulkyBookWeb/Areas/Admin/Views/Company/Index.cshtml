<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">
                Company list
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Company" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-circle-fill"></i> &nbsp; Create New
            </a>
        </div>
    </div>
</div>
<table id="tblData" class="table table-bordered table-striped w-100">
    <thead>
        <tr>
            <th style="text-align:center">Name</th>
            <th style="text-align:center">Street Address</th>
            <th style="text-align:center">City</th>
            <th style="text-align:center">State</th>
            <th style="text-align:center">PostCode</th>
            <th style="text-align:center">Phone Number</th>
            <th></th>
        </tr>
    </thead>
</table>

@section Scripts{
    <script src="~/js/company.js"></script>
}